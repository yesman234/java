
public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double radius;
		double area;
		//Assign a radius
		radius = 20;
		//compute area
		area = radius * radius * 3.14159;
		//display results
		System.out.println("The Area  for the circle radius" + radius + "is" + area);

	}

}
